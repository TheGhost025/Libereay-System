@model Libereay_System.Models.ApplicationUser

<h2>Profile Details</h2>

<div>
    <h4>@Model.FirstName @Model.LastName</h4>
    <p><strong>Email:</strong> @Model.Email</p>
    <p><strong>Role:</strong> @Model.Role</p>
</div>

<a asp-action="EditProfile" asp-controller="Admin" class="btn btn-warning">Edit Profile</a>
